<p id="notice"><%= notice %></p>

<h1>Own Vessels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @own_vessels.each do |own_vessel| %>
      <tr>
        <td><%= link_to 'Show', own_vessel %></td>
        <td><%= link_to 'Edit', edit_own_vessel_path(own_vessel) %></td>
        <td><%= link_to 'Destroy', own_vessel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Own Vessel', new_own_vessel_path %>
