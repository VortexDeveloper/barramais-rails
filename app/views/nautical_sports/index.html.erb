<p id="notice"><%= notice %></p>

<h1>Nautical Sports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nautical_sports.each do |nautical_sport| %>
      <tr>
        <td><%= link_to 'Show', nautical_sport %></td>
        <td><%= link_to 'Edit', edit_nautical_sport_path(nautical_sport) %></td>
        <td><%= link_to 'Destroy', nautical_sport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nautical Sport', new_nautical_sport_path %>
